{
  "name": "angular-webpack-quickstart",
  "version": "0.2.0",
  "description": "Simple starter for AngularJS and Bootstrap using Webpack.",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js --progress --profile --bail",
